# q_class-missing-parent-path-to-bfo.rq
# Query to find class missing parent path to BFO:Entity (obo:BFO_0000001) in English

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX cco: <https://www.commoncoreontologies.org/>
PREFIX obo: <http://purl.obolibrary.org/obo/>

SELECT ?resource ?violation
WHERE {
  ?resource a owl:Class .
  FILTER (?resource != owl:Class)
  FILTER NOT EXISTS {
    ?resource rdfs:subClassOf+ obo:BFO_0000001 .
  }
  BIND("Class is not a subclass of BFO:entity" AS ?violation)
}